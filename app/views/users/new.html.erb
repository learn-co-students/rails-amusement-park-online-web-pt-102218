<%=form_for(@user, url: {action: "create"}) do |f|%>

<%=f.label :name%>
<%=f.text_field :name %>
<br><br>

<%=f.label :password%>
<%=f.text_field :password%>
<br><br>

<%=f.label :height%>
<%=f.text_field :height%>
<br><br>

<%=f.label :happiness%>
<%=f.text_field :happiness%>
<br><br>

<%=f.label :nausea%>
<%=f.text_field :nausea%>
<br><br>

<%=f.label :tickets%>
<%=f.text_field :tickets%>
<br><br>

<%=f.label :admin%>
<%=f.check_box :admin%>
<br><br>

<%=f.submit "Create User"%>
<%end%>